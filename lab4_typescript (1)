]0;elin11@gsuad.gsu.edu@snowball:~ [elin11@gsuad.gsu.edu@snowball ~]$ cat lab4.c
#include <stdio.h>
#include <math.h>

int main() {
    float x1, y1, x2, y2, distance;
    printf("x1 = , y1 = ");
    scanf("%f %f", &x1, &y1);

    printf("x2 = , y2 = ");
    scanf("%f %f", &x2, &y2);

    distance = sqrt(pow((x2 - x1), 2) + pow((y2 - y1), 2));

    printf("Distance between the points: %.2f units\n", distance);

    return 0;
}

